  </servlet>

  <servlet-mapping>
    <servlet-name>controller.Cuenta</servlet-name>
    <url-pattern>/cuenta</url-pattern>
  </servlet-mapping>

      <servlet>
      <servlet-name>controller.Nequi</servlet-name>
    <servlet-class>controller.Nequi</servlet-class>
  </servlet>

  <servlet-mapping>
    <servlet-name>controller.Nequi</servlet-name>
    <url-pattern>/nequi</url-pattern>
  </servlet-mapping>

      <servlet>
      <servlet-name>controller.Daviplata</servlet-name>
    <servlet-class>controller.Daviplata</servlet-class>
  </servlet>

  <servlet-mapping>
    <servlet-name>controller.Daviplata</servlet-name>
    <url-pattern>/daviplata</url-pattern>
  </servlet-mapping>
</web-app>
